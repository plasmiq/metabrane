var substrate = $(".substrate_<%= @substrate.id %>");

var container = substrate.closest(".container");
var object = substrate.closest(".object");
var side = "left";
if( ! object.hasClass( "left" ) ) {
  side = "right";
}

container.find(".substrate_delete."+side).click();
container.find(".content").animate( { left: 0 }, { duration: 'slow' });
