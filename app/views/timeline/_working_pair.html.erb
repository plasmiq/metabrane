<div class="working_pair">
  <div class="timestamp"><span>created <%= time_ago_in_words( pair.updated_at ) %></span></div>
  <div class="content">
    <div class="object left">
      <%= image_tag pair.image1_url %>
    </div>
    <div  class="object right">
      <%= image_tag pair.image2_url %>
    </div>
  </div>
  <div class="relation">
    <span><%= pair.relation %></span>
    <a href="/">
      <%= image_tag "btn_clusterList.png" %>
    </a>
  </div>
</div>
